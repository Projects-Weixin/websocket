/* pages/components/answerresultssheet/answerresultsnav.wxss */

.view {
  width: 100%;
  height: 60px;
}

.navView {
  width: 100%;
  height: 60px;
  background: rgba(247, 248, 249, 1);
  display: flex;
  justify-content: center;
  align-items: center;
}

.type0-item-space {
  margin-right: 28px;
}

.type0-itemView {
  display: flex;
  align-items: center;
}

.type0-item {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 15px;
}

.type1-item-space {
  margin-right: 24px;
}

.icon-text-space {
  margin-right: 6px;
}

.navOptionView {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 115px;
  height: 34px;
  background: rgba(255, 255, 255, 1);
  border-radius: 20px;
  border: 1px solid rgba(238, 238, 238, 1);
  /* margin-right: 24px; */
}

.navOptionImage {
  background-color: gray;
  width: 20px;
  height: 18px;
}

.navTrueView {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 57px;
  height: 20px;
}

.navFalseView {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 57px;
  height: 20px;
}

.icon {
  width: 20px;
  height: 20px;
}

.text {
  font-size: 14px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 14px;
}
