/* pages/commponents/answersheet/answersheet.wxss */

.answer-sheet-page {
  width: 100%;
  height: 100%;
  background-color: white;
  z-index: 20000;
  position: fixed;
}

.title {
  margin-left: 24px;
  margin-top: 24px;
  width: 60px;
  height: 20px;
  font-size: 20px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 20px;
}

/* .content {
  display: justify-content;
  align-content: space-between;
} */

.scrollHeight {
  width: 100%;
  height: 80%;
}

.cancel {
  position: fixed;
  background-color: white;
  bottom: 0;
  height: 60px;
  width: 100%;
  border-top: 1px solid rgba(238, 238, 238, 1);
  display: flex;
  justify-content: center;
  align-items: center;
}

.cancelImage {
  width: 16px;
  height: 16px;
}
